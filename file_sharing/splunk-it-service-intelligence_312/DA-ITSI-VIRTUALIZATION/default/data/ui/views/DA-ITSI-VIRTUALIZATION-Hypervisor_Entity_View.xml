<dashboard script="DA-ITSI-VIRTUALIZATION-Hypervisor_Entity_View.js" isVisible="False">
   <label>Hypervisor Details</label>
   <row>
      <panel>
         <viz type="SA-ITSI-CustomModuleViz.entity_info_header">
           <option name="link.visible">false</option>
           <option name="refresh.time.visible">false</option>
           <search>
             <query>| savedsearch DA-ITSI-VIRTUALIZATION-Hypervisor_Inventory_Search hypervisor_id="$hypervisor_id$"</query>
             <earliest>-4d</earliest>
             <latest>now</latest>
           </search>
         </viz>
         <viz type="SA-ITSI-CustomModuleViz.mini_chart">
           <option name="SA-ITSI-CustomModuleViz.mini_chart.id">dhost-events</option>
           <option name="SA-ITSI-CustomModuleViz.mini_chart.title">Hypervisor Events (count)</option>
           <option name="SA-ITSI-CustomModuleViz.mini_chart.subtitleField">_total</option>
           <option name="SA-ITSI-CustomModuleViz.mini_chart.chartType">column</option>
           <search>
             <query>| savedsearch DA-ITSI-VIRTUALIZATION-Hypervisor_Context_Chart_Event_Search hypervisor_id="$hypervisor_id$"</query>
             <earliest>$earliest$</earliest>
             <latest>$latest$</latest>
           </search>
         </viz>
         <viz type="SA-ITSI-CustomModuleViz.mini_chart">
           <option name="SA-ITSI-CustomModuleViz.mini_chart.id">cpu-history</option>
           <option name="SA-ITSI-CustomModuleViz.mini_chart.title">CPU Usage (%)</option>
           <option name="SA-ITSI-CustomModuleViz.mini_chart.subtitleField">_stats</option>
           <option name="SA-ITSI-CustomModuleViz.mini_chart.chartType">area</option>
           <search>
             <query>| savedsearch DA-ITSI-VIRTUALIZATION-Hypervisor_Context_Chart_CPU_Usage_Search hypervisor_id="$hypervisor_id$"</query>
             <earliest>$earliest$</earliest>
             <latest>$latest$</latest>
           </search>
         </viz>
         <viz type="SA-ITSI-CustomModuleViz.mini_chart">
           <option name="SA-ITSI-CustomModuleViz.mini_chart.id">memory-history</option>
           <option name="SA-ITSI-CustomModuleViz.mini_chart.title">Memory Usage (%)</option>
           <option name="SA-ITSI-CustomModuleViz.mini_chart.subtitleField">_stats</option>
           <option name="SA-ITSI-CustomModuleViz.mini_chart.chartType">area</option>
           <search>
             <query>| savedsearch DA-ITSI-VIRTUALIZATION-Hypervisor_Context_Chart_Memory_Usage_Search hypervisor_id="$hypervisor_id$"</query>
             <earliest>$earliest$</earliest>
             <latest>$latest$</latest>
           </search>
         </viz>
      </panel>
   </row>
   <row id="tabs">
      <panel id="tabs_panel">
         <viz type="SA-ITSI-CustomModuleViz.tab_viz">
           <option name="link.visible">false</option>
           <option name="refresh.time.visible">false</option>
           <search>
             <query>|noop | stats count</query>
           </search>
         </viz>
      </panel>
   </row>
   <row depends="$control_cpu$">
      <panel ref="hypervisor_cpu_usage_percent" app="DA-ITSI-VIRTUALIZATION" id="hypervisor_cpu_usage_percent" />
      <panel ref="hypervisor_cpu_demand" app="DA-ITSI-VIRTUALIZATION" id="hypervisor_cpu_demand" />
      <panel ref="hypervisor_cpu_allocation_percent" app="DA-ITSI-VIRTUALIZATION" id="hypervisor_cpu_allocation_percent" />
   </row>
   <row depends="$control_cpu$">
      <panel ref="hypervisor_vm_cpu_load_percent_list" app="DA-ITSI-VIRTUALIZATION" id="vmcpupercenttable_panel" />
      <panel ref="hypervisor_vm_cpu_demand_list" app="DA-ITSI-VIRTUALIZATION" id="vmcpudemandtable_panel" />
   </row>
   <row depends="$control_memory$">
      <panel ref="hypervisor_memory_usage_percent" app="DA-ITSI-VIRTUALIZATION" id="hypervisor_memory_usage_percent" />
      <panel ref="hypervisor_memory_provisioned" app="DA-ITSI-VIRTUALIZATION" id="hypervisor_memory_provisioned" />
      <panel ref="hypervisor_memory_pages" app="DA-ITSI-VIRTUALIZATION" id="hypervisor_memory_pages" />
   </row>
   <row depends="$control_memory$">
      <panel ref="hypervisor_vm_mem_usage_percent_list" app="DA-ITSI-VIRTUALIZATION" id="vmmemusepcnttable_panel" />
      <panel ref="hypervisor_vm_mem_provisioned_list" app="DA-ITSI-VIRTUALIZATION" id="vmmemprovisiontable_panel" />
      <panel ref="hypervisor_vm_mem_reserved_list" app="DA-ITSI-VIRTUALIZATION" id="vmmemreservedtable_panel" />
   </row>
   <row depends="$control_network$">
      <panel ref="hypervisor_nic_ids" app="DA-ITSI-VIRTUALIZATION" id="hypervisor_nic_ids" />
      <panel ref="hypervisor_network_usage" app="DA-ITSI-VIRTUALIZATION" id="hypervisor_network_usage" />
   </row>
   <row depends="$control_network$">
      <panel ref="hypervisor_vm_network_usage_list" app="DA-ITSI-VIRTUALIZATION" id="vmnetworkusagetable_panel" />
      <panel ref="hypervisor_network_packets_dropped" app="DA-ITSI-VIRTUALIZATION" id="hypervisor_network_packets_dropped" />
   </row>
   <row depends="$control_storage$">
      <panel ref="hypervisor_datastore_list" app="DA-ITSI-VIRTUALIZATION" id="hypervisordstable_panel" />
      <panel ref="hypervisor_storage_usage" app="DA-ITSI-VIRTUALIZATION" id="hypervisor_storage_usage" />
   </row>
   <row depends="$control_storage$">
      <panel ref="hypervisor_storage_highest_latency" app="DA-ITSI-VIRTUALIZATION" id="hypervisor_storage_highest_latency" />
      <panel ref="hypervisor_storage_read_write_latency" app="DA-ITSI-VIRTUALIZATION" id="hypervisor_storage_read_write_latency" />
   </row>
   <row depends="$control_storage$">
      <panel ref="hypervisor_vm_highest_latency_list" app="DA-ITSI-VIRTUALIZATION" id="vmhighlatencytable_panel" />
      <panel ref="hypervisor_vm_storage_used_percent_list" app="DA-ITSI-VIRTUALIZATION" id="vmstorageusagetable_panel" />
   </row>
   <row depends="$control_vm$">
      <panel ref="hypervisor_vm_list" app="DA-ITSI-VIRTUALIZATION" id="hypervisorvmtable_panel" />
   </row>
   <row depends="$control_hypervisor_events$">
      <panel ref="hypervisor_events" app="DA-ITSI-VIRTUALIZATION" id="hypervisor_events" />
   </row>
</dashboard>
