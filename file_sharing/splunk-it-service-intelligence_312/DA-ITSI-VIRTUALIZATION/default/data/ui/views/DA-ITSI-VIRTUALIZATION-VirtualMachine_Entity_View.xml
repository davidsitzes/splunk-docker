<dashboard script="DA-ITSI-VIRTUALIZATION-VirtualMachine_Entity_View.js" isVisible="False">
   <label>Virtual Machine Details</label>
   <row>
      <panel>
         <viz type="SA-ITSI-CustomModuleViz.entity_info_header">
           <option name="link.visible">false</option>
           <option name="refresh.time.visible">false</option>
           <search>
             <query>| savedsearch DA-ITSI-VIRTUALIZATION-Virtual_Machine_Inventory_Search vm_id="$vm_id$"</query>
             <earliest>-4d</earliest>
             <latest>now</latest>
           </search>
         </viz>
         <viz type="SA-ITSI-CustomModuleViz.mini_chart">
           <option name="SA-ITSI-CustomModuleViz.mini_chart.id">dhost-events</option>
           <option name="SA-ITSI-CustomModuleViz.mini_chart.title">Hypervisor Events (count)</option>
           <option name="SA-ITSI-CustomModuleViz.mini_chart.subtitleField">_total</option>
           <option name="SA-ITSI-CustomModuleViz.mini_chart.chartType">column</option>
           <search>
             <query>| savedsearch DA-ITSI-VIRTUALIZATION-Virtual_Machine_Context_Chart_Event_Search vm_id="$vm_id$"</query>
             <earliest>$earliest$</earliest>
             <latest>$latest$</latest>
           </search>
         </viz>
         <viz type="SA-ITSI-CustomModuleViz.mini_chart">
           <option name="SA-ITSI-CustomModuleViz.mini_chart.id">cpu-history</option>
           <option name="SA-ITSI-CustomModuleViz.mini_chart.title">CPU Usage (%)</option>
           <option name="SA-ITSI-CustomModuleViz.mini_chart.subtitleField">_stats</option>
           <option name="SA-ITSI-CustomModuleViz.mini_chart.chartType">area</option>
           <search>
             <query>| savedsearch DA-ITSI-VIRTUALIZATION-Virtual_Machine_Context_Chart_CPU_Usage_Search vm_id="$vm_id$"</query>
             <earliest>$earliest$</earliest>
             <latest>$latest$</latest>
           </search>
         </viz>
         <viz type="SA-ITSI-CustomModuleViz.mini_chart">
           <option name="SA-ITSI-CustomModuleViz.mini_chart.id">memory-history</option>
           <option name="SA-ITSI-CustomModuleViz.mini_chart.title">Memory Usage (%)</option>
           <option name="SA-ITSI-CustomModuleViz.mini_chart.subtitleField">_stats</option>
           <option name="SA-ITSI-CustomModuleViz.mini_chart.chartType">area</option>
           <search>
             <query>| savedsearch DA-ITSI-VIRTUALIZATION-Virtual_Machine_Context_Chart_Memory_Usage_Search vm_id="$vm_id$"</query>
             <earliest>$earliest$</earliest>
             <latest>$latest$</latest>
           </search>
         </viz>
         <viz type="SA-ITSI-CustomModuleViz.mini_chart">
           <option name="SA-ITSI-CustomModuleViz.mini_chart.id">storage-history</option>
           <option name="SA-ITSI-CustomModuleViz.mini_chart.title">Storage Usage (%)</option>
           <option name="SA-ITSI-CustomModuleViz.mini_chart.subtitleField">_stats</option>
           <option name="SA-ITSI-CustomModuleViz.mini_chart.chartType">area</option>
           <search>
             <query>| savedsearch DA-ITSI-VIRTUALIZATION-Virtual_Machine_Context_Chart_Storage_Usage_Search vm_id="$vm_id$"</query>
             <earliest>$earliest$</earliest>
             <latest>$latest$</latest>
           </search>
         </viz>
      </panel>
   </row>
   <row id="tabs">
      <panel id="tabs_panel">
         <viz type="SA-ITSI-CustomModuleViz.tab_viz">
           <option name="link.visible">false</option>
           <option name="refresh.time.visible">false</option>
           <search>
             <query>|noop | stats count</query>
           </search>
         </viz>
      </panel>
   </row>
   <row depends="$control_cpu$">
      <panel ref="vm_cpu_usage_percent" app="DA-ITSI-VIRTUALIZATION" id="vm_cpu_usage_percent" />
      <panel ref="vm_cpu_demand" app="DA-ITSI-VIRTUALIZATION" id="vm_cpu_demand" />
   </row>
   <row depends="$control_memory$">
      <panel ref="vm_memory_usage_percent" app="DA-ITSI-VIRTUALIZATION" id="vm_memory_usage_percent" />
      <panel ref="vm_memory_provisioned" app="DA-ITSI-VIRTUALIZATION" id="vm_memory_provisioned" />
      <panel ref="vm_memory_reserved" app="DA-ITSI-VIRTUALIZATION" id="vm_memory_reserved" />
   </row>
   <row depends="$control_network$">
      <panel ref="vm_nic_ids" app="DA-ITSI-VIRTUALIZATION" id="vm_nic_ids" />
      <panel ref="vm_network_usage" app="DA-ITSI-VIRTUALIZATION" id="vm_network_usage" />
   </row>
   <row depends="$control_network$">
      <panel ref="vm_network_packets_dropped" app="DA-ITSI-VIRTUALIZATION" id="vm_network_packets_dropped" />
   </row>
   <row depends="$control_storage$">
      <panel ref="vm_datastore_list" app="DA-ITSI-VIRTUALIZATION" id="vmdstable_panel" />
   </row>
   <row depends="$control_storage$">
      <panel ref="vm_storage_highest_latency" app="DA-ITSI-VIRTUALIZATION" id="vm_storage_highest_latency" />
      <panel ref="vm_storage_read_write_latency" app="DA-ITSI-VIRTUALIZATION" id="vm_storage_read_write_latency" />
   </row>
   <row depends="$control_storage$">
      <panel ref="vm_storage_usage" app="DA-ITSI-VIRTUALIZATION" id="vm_storage_usage" />
      <panel ref="vm_storage_used_percent" app="DA-ITSI-VIRTUALIZATION" id="vm_storage_used_percent" />
   </row>
   <row depends="$control_vm_events$">
      <panel ref="vm_events" app="DA-ITSI-VIRTUALIZATION" id="vm_events" />
   </row>
</dashboard>