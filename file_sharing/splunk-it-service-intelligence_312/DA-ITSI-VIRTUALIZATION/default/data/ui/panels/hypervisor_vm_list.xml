<panel>
  <title>List of Virtual Machines</title>
  <input type="text" token="vm_name" searchWhenChanged="true">
    <label>Search Virtual Machine</label>
    <default></default>
  </input>
  <table id="hypervisorvmtable">
    <search>
      <query>| savedsearch DA-ITSI-VIRTUALIZATION-Hypervisor_List_Connected_VM_Search hypervisor_id="$hypervisor_id$" vm_name="$vm_name$"</query>
      <earliest>$earliest$</earliest>
      <latest>$latest$</latest>
    </search>
  </table>
</panel>
